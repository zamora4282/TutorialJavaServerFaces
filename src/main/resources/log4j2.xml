<?xml version="1.0" encoding="UTF-8"?>
<configuration status="INFO">
    <Appenders>
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm:ss} [%t] %-5level %looger{36} -%msg%n"></PatternLayout>
        </Console>
    </Appenders>
    <Loogers>
        <Root level="info">
            <AppenderRef ref="Console" />
        </Root>
    </Loogers>
</configuration>
